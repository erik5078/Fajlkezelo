<Window x:Class="FajlKezelo.View.UjMappaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Új mappa" Height="154" Width="293" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="ujMappaLabel" Content="Új mappa" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="ujMappa_textBox" HorizontalAlignment="Left" Height="23" Margin="10,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="254" Text="{Binding UjMappa}" />
        <Button x:Name="OK_button" Content="OK" HorizontalAlignment="Left" Margin="87,94,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.44,0.55" Click="OK_button_Click" />
        <Button x:Name="Megse_button" Content="Mégse" HorizontalAlignment="Left" Margin="189,94,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.44,0.55" Click="Megse_button_Click"/>
    </Grid>
</Window>
